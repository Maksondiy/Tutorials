<!DOCTYPE html>
<html>
<head>
    <!-- Slick Carousel CSS -->
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css">
    <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick-theme.css">

    <link rel="stylesheet" href="/Css/Css.css">
    <script src="/Js/JavaScript.js"></script>
    <!-- Slick Carousel JavaScript -->
    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
    <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js"></script>

</head>
<body>
    <div class="slider-container">
        <div class="slider">
            <div class="slide">
                <img src="/1.png" alt="Image 1">
            </div>
            <div class="slide">
                <img src="/2.png" alt="Image 2">
            </div>
            <div class="slide">
                <img src="/1.png" alt="Image 2">
            </div>
            <!-- Add more slides with images as needed -->
        </div>
    </div>
    <script>
        $(document).ready(function () {
            $('.slider').slick({
                autoplay: true, // Enable auto-sliding
                autoplaySpeed: 700, // Set the auto-slide interval in milliseconds
                arrows: true, // Display navigation arrows (optional)
                dots: true, // Display navigation dots (optional)
            });
        });
    </script>

</body>
</html>



